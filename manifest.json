{
    "manifest_version": 3,
    "name": "TabSwitch",
    "version": "1.0.0",
    "description": "Switch from chrome tabs.",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_title": "TabSwitch",
        "default_icon": "icons/32.png",
        "default_popup": "index.html"
    },
    "permissions": [
       "tabs"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
  
    "minimum_chrome_version": "88"
}